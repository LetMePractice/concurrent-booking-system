# Application
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/event_booking
DATABASE_URL_SYNC=postgresql://postgres:postgres@localhost:5432/event_booking
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=10

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_ENABLED=true
REDIS_CACHE_TTL=300

# JWT Auth â€” CHANGE THIS IN PRODUCTION
SECRET_KEY=super-secret-key-change-in-production-use-openssl-rand-hex-32
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Metrics
METRICS_ENABLED=true
